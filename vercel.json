{
    "version": 2,
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/", "dest": "/index.html" },
      { "src": "/pages/(.*)", "dest": "/pages/$1" },
      { "src": "/public/(.*)", "dest": "/public/$1" }
    ]
  }
  