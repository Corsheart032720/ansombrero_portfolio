{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "pages/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/inworke-project",
      "dest": "/pages/inworke-project.html"
    },
    {
      "src": "/pilock-project",
      "dest": "/pages/pilock-project.html"
    },
    {
      "src": "/pos-project",
      "dest": "/pages/pos-project.html"
    }
  ]
}
